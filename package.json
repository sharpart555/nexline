{
  "name": "nexline",
  "version": "1.2.2",
  "description": "Read file, stream, string, buffer line by line without putting them all in memory. It supports cool features like `custom line separator`, `various encodings`, `reverse mode`, `iterable protocol`",
  "author": "Youngho Jin",
  "license": "MIT",
  "homepage": "https://github.com/sharpart555/nexline",
  "repository": {
    "type": "git",
    "url": "https://github.com/sharpart555/nexline"
  },
  "bugs": {
    "url": "https://github.com/sharpart555/nexline/issues"
  },
  "keywords": [
    "nexline",
    "readline",
    "next",
    "line",
    "nextline",
    "next-line",
    "line-next",
    "linebyline",
    "stream",
    "string",
    "buffer",
    "file",
    "read",
    "reverse",
    "iterable"
  ],
  "main": "index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "fs-extra": "^9.0.0",
    "iconv-lite": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "scripts": {
    "eslint": "eslint \"./**/*.js\"",
    "test": "mocha ./test",
    "test:node8": "mocha ./test --exclude test/iterable.test.js"
  }
}
